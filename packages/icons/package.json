{
  "name": "@cjhsyc/icons",
  "version": "0.1.1",
  "license": "MIT",
  "author": "cjhsyc",
  "keywords": [
    "icons",
    "vue",
    "svg",
    "vite"
  ],
  "homepage": "https://github.com/cjhsyc/c-icons#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/cjhsyc/c-icons.git"
  },
  "type": "module",
  "files": [
    "dist",
    "types"
  ],
  "main": "./dist/icons.umd.js",
  "module": "./dist/icons.es.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/icons.es.js",
      "require": "./dist/icons.umd.js"
    },
    "./style": "./dist/style.css"
  },
  "scripts": {
    "build": "pnpm run build:build && vite build && pnpm run build:types",
    "build:build": "npx tsx build/build.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "unplugin-icons": "^0.14.8"
  },
  "devDependencies": {
    "@types/node": "^18.6.4",
    "@types/prettier": "^2.7.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/compiler-sfc": "^3.2.37",
    "prettier": "^2.7.1",
    "tsx": "^3.8.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vue": "^3.2.37",
    "vue-tsc": "^0.38.4"
  }
}
